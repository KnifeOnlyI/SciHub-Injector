{
  "name": "Sci-Hub Injector",
  "description": "A webpage injector of direct access sci-hub link",
  "version": "1.0.0",
  "permissions": [],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://pubmed.ncbi.nlm.nih.gov/*",
        "https://www.nature.com/*",
        "https://www.sciencedirect.com/*",
        "https://www.tandfonline.com/*",
        "https://link.springer.com/*",
        "https://www.cochranelibrary.com/*",
        "https://scholar.google.com/*",
        "https://ieeexplore.ieee.org/*",
        "https://www.bmj.com/*",
        "https://www.eurekaselect.com/*"
      ],
      "js": [
        "dist/bundle.js"
      ]
    }
  ],
  "icons": {
    "128": "resources/icons/icon_128.png",
    "48": "resources/icons/icon_48.png",
    "32": "resources/icons/icon_32.png",
    "16": "resources/icons/icon_16.png"
  }
}
